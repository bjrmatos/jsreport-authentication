    "shortid": "2.2.4",
    "string": "3.3.1",
    "underscore": "1.8.3"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "eslint": "^1.9.0",
    "jsreport-core": "*",
    "jsreport-express": "*",
    "mocha": "^2.3.3",
